{
  "name": "schl-web",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "turbo": "^2.6.0"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9.0.6"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@types/cookie-parser": "^1.4.10",
    "cookie-parser": "^1.4.7"
  }
}
